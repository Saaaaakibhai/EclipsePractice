
public class Student extends Person {
	private int ID;
	private double cgpa;

	Student() {

	}

	Student(String name, int age, String address, int ID, double cgpa) {
		super(name, age, address);
		this.ID = ID;
		this.cgpa = cgpa;
	}

	public int getID() {
		return ID;
	}

	public double getCgpa() {
		return cgpa;
	}

	public void setID(int iD) {
		ID = iD;
	}

	public void setCgpa(double cgpa) {
		this.cgpa = cgpa;
	}

	@Override
	public String toString() {
		return "Student Name: " + this.getName() + "\nAge: " + this.getAge() + "\nAddress: " + this.getAddress()
				+ "\nID: " + this.getID() + "\nCGPA: " + this.getCgpa() + "\n";
	}

}
