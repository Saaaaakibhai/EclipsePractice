import java.util.Scanner;

public class sec3{
	 public static void main(String args[]){
	 int i,j,n;
	 Scanner input = new Scanner(System.in);
	     System.out.println("Enter the no of lines");
	     n=input.nextInt();
	     for(i=1;i<=n;i++){	 
	        for(j=1;j<=n-i;j++){
	     System.out.printf(" ");
} 
	   
	        for(j=i-1;j>=1;j--) {
	     System.out.printf("%d",j);
} 
	     System.out.println();
         }
	 }
}
